const frameworks: Array<CompatibilityItem> = [
    {
    title: "Payment History",
    percent: "35%",

   },
    {
    title: "Amount Owned",
    percent: "30%",

  },
    {
    title: "Length of Credit History",
    percent: "15%",

  },
  {
